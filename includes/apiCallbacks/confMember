</code></pre>
<pre class="highlight json"><code><span class="p">{</span><span class="w">
    </span><span class="nt">"activeMembers"</span><span class="p">:</span><span class="w"> </span><span class="mi">1</span><span class="p">,</span><span class="w">
    </span><span class="nt">"callId"</span><span class="p">:</span><span class="w"> </span><span class="s2">"{callId}"</span><span class="p">,</span><span class="w">
    </span><span class="nt">"conferenceId"</span><span class="p">:</span><span class="w"> </span><span class="s2">"{conferenceId}"</span><span class="p">,</span><span class="w">
    </span><span class="nt">"eventType"</span><span class="p">:</span><span class="w"> </span><span class="s2">"conference-member"</span><span class="p">,</span><span class="w">
    </span><span class="nt">"hold"</span><span class="p">:</span><span class="w"> </span><span class="kc">false</span><span class="p">,</span><span class="w">
    </span><span class="nt">"memberId"</span><span class="p">:</span><span class="w"> </span><span class="s2">"{memberId}"</span><span class="p">,</span><span class="w">
    </span><span class="nt">"memberUri"</span><span class="p">:</span><span class="w"> </span><span class="s2">"https://api.catapult.inetwork.com/v1/users/{userId}/conferences/{conferenceId}/members/{memberId}"</span><span class="p">,</span><span class="w">
    </span><span class="nt">"mute"</span><span class="p">:</span><span class="w"> </span><span class="kc">true</span><span class="p">,</span><span class="w">
    </span><span class="nt">"state"</span><span class="p">:</span><span class="w"> </span><span class="s2">"active"</span><span class="p">,</span><span class="w">
    </span><span class="nt">"time"</span><span class="p">:</span><span class="w"> </span><span class="s2">"2013-07-12T20:53:11.646Z"</span><span class="w">
</span><span class="p">}</span><span class="w">
</span></code></pre>
